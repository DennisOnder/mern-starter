{
  "name": "mern-starter-kit",
  "version": "1.0.0",
  "description": "MERN Stack Starter Kit",
  "main": "express.js",
  "scripts": {
    "install-dependencies": "npm install && npm install --prefix client",
    "server": "nodemon express",
    "dev": "concurrently \"npm run server\" \"npm run start --prefix client\"",
    "dev-sass": "concurrently \"npm run server\" \"npm run dev --prefix client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennisOnder/mern-starter.git"
  },
  "keywords": [
    "MERN"
  ],
  "author": "Dennis Onder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DennisOnder/mern-starter-kit/issues"
  },
  "homepage": "https://github.com/DennisOnder/mern-starter-kit#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}